@model Ogdi.InteractiveSdk.Mvc.Models.Rating.VoteResults
@{
    Layout = null;
    }

<div class="numbers">
    @{ if (this.Model.Positive > 0)
       { 
        <span class="positive">+ @Model.Positive</span>
           if (this.Model.Negative > 0)
           { 
        <text>/</text>	<span class="negative">- @Model.Negative</span>
        }
       }
       else if (this.Model.Negative > 0)
       { 
        <span class="negative">- @Model.Negative</span>
     }
       else
       { 
        <span class="not-rated">@SharedResources.RatesPlusMinus.NotRated </span>
     } }
</div>

